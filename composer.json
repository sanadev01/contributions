{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "authorizenet/authorizenet": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "doctrine/dbal": "^2.12.1",
        "geekcom/validator-docs": "^3.5",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.3",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2.1",
        "league/flysystem": "~3.12.3",
        "livewire/livewire": "^2.3",
        "milon/barcode": "^10.0",
        "phpoffice/phpspreadsheet": "^1.14",
        "picqer/php-barcode-generator": "^2.2",
        "rap2hpoutre/laravel-log-viewer": "^2.3.0",
        "saqijaan/laravel-json-column-trait": "dev-master",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "spatie/laravel-activitylog": "^4.7",
        "stripe/stripe-php": "^7.100",
        "webklex/laravel-pdfmerger": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "AmazonSellingPartner\\": "app/Libraries/AmazonSellingPartner"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Http/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
